{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "id",
      "title",
      "body"
    ],
    "properties": {
      "title": {
        "type": "string",
        "minLength": 3
      },
      "body": {
        "type": "string",
        "minLength": 10
      },
      "labels": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "id": {
        "type": "string",
        "minLength": 3,
        "maxLength": 64
      }
    },
    "additionalProperties": false
  }
}